<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape android:shape="rectangle">
			<solid android:color="@color/app_default_btn_pressed_color"/>
			<corners android:radius="10dp"/>
			<stroke
				android:width="1dp"
				android:color="@color/app_default_btn_pressed_color"/>
		</shape>
	</item>
	<item android:state_enabled="true">
		<shape android:shape="rectangle">
			<solid android:color="@color/app_default_alpha_color_background"/>
			<corners android:radius="10dp"/>
			<stroke
				android:width="1dp"
				android:color="@color/app_default_alpha_color_background"/>
		</shape>
	</item>
</selector>